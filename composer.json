{
    "name": "quanta/dependency-injection",
    "type": "library",
    "description": "Dependency injection library for php",
    "keywords": [
        "psr-11",
        "container",
        "dependency-injection",
        "injection"
    ],
    "license": "MIT",
    "homepage": "https://github.com/quantaphp/dependency-injection",
    "support": {
        "issues": "https://github.com/quantaphp/dependency-injection/issues",
        "source": "https://github.com/quantaphp/dependency-injection"
    },
    "authors": [
        {
            "name": "pmall",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.0",
        "psr/container": "^1.0",
        "quanta/exceptions": "^0.4.0",
        "quanta/partial-application": "^0.4.0"
    },
    "require-dev": {
        "eloquent/phony-kahlan": "^2.0",
        "kahlan/kahlan": "^4.0",
        "phpstan/phpstan": "^0.11.0"
    },
    "autoload": {
        "psr-4": {
            "Quanta\\DI\\": "src"
        }
    }
}
